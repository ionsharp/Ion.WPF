<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Ion.Controls">
  <Top/>
  <Left/>
  <Right>
	  <Panel Name="NotificationPanel"/>
	  <Panel Name="ThemePanel"/>
  </Right>
  <Bottom/>
  <Root xsi:type="DockLayoutGroup" Orientation="Horizontal">
    <Elements>
      <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="6*">
        <Elements>
          <Element xsi:type="DockLayoutDocumentGroup" Height="65*"/>
          <Element xsi:type="DockLayoutPanelGroup" Height="35*">
            <Panels>
				<DockLayoutPanel IsSelected="true" Name="LogPanel"/>
				<DockLayoutPanel Name="ColorPanel"/>
				<DockLayoutPanel Name="GradientPanel"/>
				<DockLayoutPanel Name="ColorHarmonyPanel"/>
				<DockLayoutPanel Name="MatrixPanel"/>
				<DockLayoutPanel Name="ProfilePanel"/>
				<DockLayoutPanel Name="IlluminantPanel"/>
				<DockLayoutPanel Name="ColorAnalysisPanel"/>
            </Panels>
          </Element>
        </Elements>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="4*">
        <Panels>
			<DockLayoutPanel IsSelected="true" Name="PropertyPanel"/>
			<DockLayoutPanel Name="ColorChromacityPanel"/>
			<DockLayoutPanel Name="ColorDifferencePanel"/>
			<DockLayoutPanel Name="ClipboardPanel"/>
			<DockLayoutPanel Name="OptionPanel"/>
		</Panels>
      </Element>
    </Elements>
  </Root>
</Layout>