<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <!--#region Double-->

    <System:Double x:Key="DoubleLarger">20</System:Double>

    <System:Double x:Key="DoubleLarge">10</System:Double>

    <System:Double x:Key="Double">5</System:Double>

    <System:Double x:Key="DoubleSmall">2.5</System:Double>

    <System:Double x:Key="DoubleSmaller">1</System:Double>

    <!--#endregion-->

    <!--#region Opacity-->

    <System:Double x:Key="OpacityDisable">0.5</System:Double>

    <System:Double x:Key="OpacityEnable">1.0</System:Double>

    <!--#endregion-->

    <!--#region Shape-->

    <Geometry x:Key="ShapeTriangleDown">M0,0 L0,2 L4,6 L8,2 L8,0 L4,4 z</Geometry>

    <Geometry x:Key="ShapeTriangleRight">M0,0 L0,8 L4,4 z</Geometry>

    <!--#endregion-->

    <!--#region Thick > 20.0 (Larger)-->

    <Thickness x:Key="ThickLarger">20</Thickness>

    <Thickness x:Key="ThickLargerDown">0,0,0,20</Thickness>

    <Thickness x:Key="ThickLargerLeft">20,0,0,0</Thickness>

    <Thickness x:Key="ThickLargerLeftRight">20,0,20,0</Thickness>

    <Thickness x:Key="ThickLargerRight">0,0,20,0</Thickness>

    <Thickness x:Key="ThickLargerUp">0,20,0,0</Thickness>

    <Thickness x:Key="ThickLargerUpDown">0,20,0,20</Thickness>

    <!--#endregion-->

    <!--#region Thick > 10.0 (Large)-->

    <Thickness x:Key="ThickLarge">10</Thickness>

    <Thickness x:Key="ThickLargeDown">0,0,0,10</Thickness>

    <Thickness x:Key="ThickLargeLeft">10,0,0,0</Thickness>

    <Thickness x:Key="ThickLargeLeftRight">10,0,10,0</Thickness>

    <Thickness x:Key="ThickLargeRight">0,0,10,0</Thickness>

    <Thickness x:Key="ThickLargeUp">0,10,0,0</Thickness>

    <Thickness x:Key="ThickLargeUpDown">0,10,0,10</Thickness>

    <!--#endregion-->

    <!--#region Thick >  5.0-->

    <Thickness x:Key="Thick">5</Thickness>

    <Thickness x:Key="ThickDown">0,0,0,5</Thickness>

    <Thickness x:Key="ThickLeft">5,0,0,0</Thickness>

    <Thickness x:Key="ThickLeftRight">5,0,5,0</Thickness>

    <Thickness x:Key="ThickRight">0,0,5,0</Thickness>

    <Thickness x:Key="ThickUp">0,5,0,0</Thickness>

    <Thickness x:Key="ThickUpDown">0,5,0,5</Thickness>

    <!--#endregion-->

    <!--#region Thick >  2.5 (Small)-->

    <Thickness x:Key="ThickSmall">2.5</Thickness>

    <Thickness x:Key="ThickSmallDown">0,0,0,2.5</Thickness>

    <Thickness x:Key="ThickSmallLeft">2.5,0,0,0</Thickness>

    <Thickness x:Key="ThickSmallLeftRight">2.5,0,2.5,0</Thickness>

    <Thickness x:Key="ThickSmallRight">0,0,2.5,0</Thickness>

    <Thickness x:Key="ThickSmallUp">0,2.5,0,0</Thickness>

    <Thickness x:Key="ThickSmallUpDown">0,2.5,0,2.5</Thickness>

    <!--#endregion-->

    <!--#region Thick >  1.0 (Smaller)-->

    <Thickness x:Key="ThickSmaller">1</Thickness>

    <Thickness x:Key="ThickSmallerDown">0,0,0,1</Thickness>

    <Thickness x:Key="ThickSmallerLeft">1,0,0,0</Thickness>

    <Thickness x:Key="ThickSmallerLeftRight">1,0,1,0</Thickness>

    <Thickness x:Key="ThickSmallerRight">0,0,1,0</Thickness>

    <Thickness x:Key="ThickSmallerUp">0,1,0,0</Thickness>

    <Thickness x:Key="ThickSmallerUpDown">0,1,0,1</Thickness>

    <!--#endregion-->
</ResourceDictionary>